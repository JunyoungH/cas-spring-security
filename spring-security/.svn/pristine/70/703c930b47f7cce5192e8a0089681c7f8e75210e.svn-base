<html class="no-js" lang="en">

<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">

<link rel="icon" type="image/ico" href="img/favicon.ico">
<script src="../js/libs/jquery.min.js"></script>
</head>

<body>

	<div>
		
		<div class="form-group">
			"Please 'Like' our page before access free WiFi"
		</div>
		<div class="form-group">
			<div class="fb-page" 
			  data-href="https://www.facebook.com/TechStudioSolutions"
			  data-width="380" 
			  data-hide-cover="false"
			  data-show-facepile="false" 
			  data-show-posts="false"></div>
		</div>
		<div>
			<input id="submit" type="button" value='Access Free WiFi' class="btn btn-primary">
		</div>
		
		
	</div>


	<script type="text/javascript">
	
		window.fbAsyncInit = function() {
			var appId = '869172546520662';
			FB.init({
			  appId      : appId,
			  status     : true,
			  xfbml      : true,
			  cookie     : true,
			  version    : 'v2.7',
			  scope      : 'user_likes'
			});
			FB.Event.subscribe('edge.create', function(response) {
				window.location.href = "http://techstudio.com.sg/";
			});
		};

		(function(d, s, id) {
		  var js, fjs = d.getElementsByTagName(s)[0];
		  if (d.getElementById(id)) return;
		  js = d.createElement(s); js.id = id;
		  js.src = "//connect.facebook.net/en_US/sdk.js";
		  fjs.parentNode.insertBefore(js, fjs);
		}(document, 'script', 'facebook-jssdk'));
		
		
	</script>

</body>
</html>

